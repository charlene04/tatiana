<%include ./partials/header.ejs %>
<%include ./partials/sidebar.ejs %>


<div class="col-md-8">
    <h1 class="text-center pb-4 pt-3">Registered Users</h1>
    <div class="row">
        <div class="col-md-9">
            <%var count = 0%>
            <%users.forEach(function(users){%>
                <ul>
                    <li><h4><strong><%=users.username%></strong></h4></li>
                    <p><%=users.email%></p>
                </ul> 
                <%count++%> 
            <%})%>
        </div>

        <div class="col-md-3">
        <p class="float-right"><strong>Total Registered Users: <%=count%></strong></p>
        </div>
    </div>
</div>
</div>

</div>

<%include ./partials/footer.ejs %>